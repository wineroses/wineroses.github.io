{
	"schemaVersion": "0.1.0",
	"name": "Felo",
	"behavior": "create",
	"noteContentFormat": "{{selectorHtml:div.prose, div.mb-6 h1 pre, .thread-item .flex-1>div>img|join:\"\\n\"|markdown}}\n",
	"properties": [
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author}}",
			"type": "multitext"
		},
		{
			"name": "published",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "created",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "image",
			"value": "{{image}}",
			"type": "text"
		}
	],
	"triggers": [
		"https://felo.ai/ja/search"
	],
	"noteNameFormat": "{{title|safe_name:mac}}",
	"path": "Clippings"
}
